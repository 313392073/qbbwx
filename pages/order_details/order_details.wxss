.detailFailureCon {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
}

.detailFailure {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 90%;
  border-radius: 8rpx;
  -webkit-transform: translateX(-50%) translateY(-50%);
  transform: translateX(-50%) translateY(-50%);
  background-color: #fff;
}

.detailFailure_title {
  text-align: center;
  font-size: 36rpx;
  font-weight: 700;
  padding: 40rpx 0 20rpx;
  
}
.chooseFailureList image{
    width: 56rpx;
    height: 56rpx;
}
.detailFailure_content {
  min-height: 100rpx;
  max-height: 600rpx;
  overflow: auto;
}

.detailFailure_content view {
  line-height: 50rpx;
  font-size: 28rpx;
 
  border-radius: 8rpx;
  width: 90%;
  margin: 10rpx auto;
  padding: 0 30rpx;
}

.detailFailure_content view.active {
  border: 1rpx solid #fc0000;
}

.detailFailure_content view.active text {
  color: #fc0000;
}

.detailFailure_content_r {
  float: right;
}

.detailFailure_content_tip {
  width: 90%;
  font-size: 24rpx;
  color: #999;
  margin: 20rpx auto;
}

.detailFailure_btn text {
  float: left;
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-weight: 36rpx;
  border-top: 1rpx solid #e4e4e4;
  color: #333;
}

.detailFailure_btn_l {
  color: #999 !important;
  border-right: 1rpx solid #e4e4e4;
}

/*其他故障弹窗*/

.detailFailure_ortherCon {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 3;
  /*display: -webkit-flex;
  -webkit-align-items: center;
  -webkit-justify-content: center;*/
}

.detailFailure_orther {
  position: absolute;
  left: 50%;
  top: 50%;
  width: 90%;
  border-radius: 8rpx;
  -webkit-transform: translateX(-50%) translateY(-50%);
  transform: translateX(-50%) translateY(-50%);
  background-color: #fff;
}

.detailFailure_orther_title {
  text-align: center;
  font-size: 36rpx;
  font-weight: 700;
  padding: 40rpx 0 20rpx;
}

.detailFailure_orther_textarea {
  width: 90%;
  border: 1rpx solid #e4e4e4;
  padding: 30rpx;
  font-size: 28rpx;
  margin: auto;
  background-color: #f8f8f8;
}

.detailFailure_orther_imgCon {
  width: 90%;
  margin: 30rpx auto;
}

.detailFailure_orther_imgCon view {
  position: relative;
  display: inline-block;
  vertical-align: top;
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  border: 1rpx solid #e4e4e4;
  margin-right: 30rpx;
}

.detailFailure_orther_imgCon view image {
  width: 100%;
  height: 100%;
}

.detailFailure_orther_imgCon view text {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 100%;
  color: #fff;
  background-color: #ccc;
}

.addImg {
  line-height: 100rpx;
  font-size: 80rpx;
  font-weight: 700;
  color: #ccc;
}

.detailFailure_orther_btn {
  border-top: 1rpx solid #e4e4e4;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin-top: 30rpx;
}


