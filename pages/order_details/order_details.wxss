page {
  font-size: 28rpx;
  color: #333;
  background-color: #efeff4;
}

.block {
  position: relative;
  margin: 25rpx 0;
  background-color: #fff;
  padding-left: 30rpx;
}

.top {
  padding: 30rpx;
}

.top_r {
  float: right;
  color: #ffda44;
}

.arrows {
  position: absolute;
  right: 50rpx;
  top: 50%;
  width: 28rpx;
  height: 28rpx;
  border-width: 1rpx;
  border-color: #999 #999 transparent transparent;
  border-style: solid;
  -webkit-transform: rotate(45deg) translateY(-50%);
  transform: rotate(45deg) translateY(-50%);
}

.box-align-center {
  -webkit-box-align: center;
  -moz-box-align: center;
}

.display-box-h {
  width: 100%;
  display: -moz-box;
  display: -webkit-box;
  display: box;
  -moz-box-orient: horizontal;
  -webkit-box-orient: horizontal;
  box-orient: horizontal;
}

.flex-one {
  -moz-box-flex: 1;
  -webkit-box-flex: 1;
  box-flex: 1;
}

.masterInfoCon {
  position: relative;
  padding: 30rpx;
}

.headerImgCon {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100%;
  overflow: hidden;
  background-color: #999;
}

.headerImgCon image {
  width: 100%;
  height: 100%;
}

.masterInfo {
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.masterPhone {
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  padding: 15rpx;
  background-color: #89ca12;
}

.masterPhone image {
  width: 100%;
  height: 100%;
}

.masterInfo_top {
  font-size: 32rpx;
  margin-bottom: 10rpx;
  margin-left: -30rpx;
}

.masterInfo_tip {
  color: #999;
  font-size: 28rpx;
}

.serverInfoCon {
  padding: 5rpx 30rpx 5rpx 30rpx;
  
}

.serverInfoCon view {
  padding: 18rpx 0;
}

.serverInfo_contact {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;

  color: #666;
}

.serverInfo_l {
  float: left;
  margin-right: -160rpx;
  font-weight: 700;
  color: #333;
  width: 160rpx;
}

.serverInfo_r {
  padding-left: 160rpx;
}

.title {
