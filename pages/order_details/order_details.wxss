/*时间蒙层*/

.timeShade {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1;
}

.timeSelectCon {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 580rpx;
  overflow: hidden;
  background-color: #fff;
}

.timeSelect_l {
  float: left;
  width: 40%;
  height: 400rpx;
  text-align: center;
  overflow-y: auto;
  background-color: #f2f2f2;
}

.timeSelect_l>view, .timeSelect_r>view {
  line-height: 80rpx;
  color: #999;
}

.timeSelect_l>view.active {
  color: #333;
  background-color: #fff;
}

.timeSelect_r>view.active {
  color: #fc0000;
}

.timeSelect_r {
  float: left;
  width: 60%;
  height: 400rpx;
  text-align: center;
  overflow-y: auto;
}

.timeSelect_btn {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 32rpx;
  text-align: center;
  border-top: 1rpx solid #e4e4e4;
}
.timeSelect_btn:active{
  background-color: #f2f2f2;
}

